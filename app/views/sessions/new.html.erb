<h1> Session Nuevo </h2>

<%=  form_tag(login_path) do %>
  <fieldset>
    <section>
      <%= label_tag(:email) %>
      <%= email_field_tag(:email) %>
      </section>
      <section>
        <%= label_tag(:password) %>
        <%= password_field_tag(:password) %>
      </section>
      <%= submit_tag("Login") %>
    </section>
  </fieldset>
  <% end %>